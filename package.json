{
  "name": "nisix-utilities",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "scully": "scully --project nisix-utilities --noPrompt --no-serve",
    "watch": "ng build --watch --configuration development",
    "image:build": "npm run build && DOCKER_BUILDKIT=1 docker build --platform amd64 -t frontend-nisix-utilities .",
    "image:deploy": "docker-compose down && docker-compose up -d",
    "test": "ng test",
    "serve:ssr:nisix-utilities": "node dist/nisix-utilities/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "19.2.2",
    "@angular/common": "19.2.2",
    "@angular/compiler": "19.2.2",
    "@angular/core": "19.2.2",
    "@angular/forms": "19.2.2",
    "@angular/material": "19.2.2",
    "@angular/platform-browser": "19.2.2",
    "@angular/platform-browser-dynamic": "19.2.2",
    "@angular/platform-server": "19.2.2",
    "@angular/router": "19.2.2",
    "@angular/ssr": "^19.2.3",
    "@scullyio/init": "^2.1.21",
    "@scullyio/ng-lib": "^2.1.41",
    "@scullyio/scully": "^2.1.41",
    "@scullyio/scully-plugin-puppeteer": "^2.1.41",
    "@types/luxon": "^3.3.5",
    "express": "^4.18.2",
    "luxon": "^3.4.4",
    "material-icons": "^1.13.14",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "^11.1.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.2.3",
    "@angular/cli": "19.2.3",
    "@angular/compiler-cli": "19.2.2",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.8.2"
  }
}
